{
  "expo": {
    "name": "covid-apptwo",
    "slug": "covid-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icontwo.png",
    "splash": {
      "image": "./assets/splashtwo.png",
      "resizeMode": "contain",
      "backgroundColor": "#8C89FA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#8C89FA"
    },
    "android":{
      "permissions":["LOCATION, NOTIFICATIONS"],
      "useNextNotificationsApi": true
    } 
  }
}